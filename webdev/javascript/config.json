{
    "init": {
        "cmd": "npm install --yes --save-dev clean-webpack-plugin && npm install --yes --save-dev webpack-cli@4.9.2",
        "env": "./azion/webdev.env",
        "output-ctrl": "on-error"
    },
    "build": {
        "cmd": "npx --yes --package=webpack@5.72.0 --yes --package=webpack-cli@4.9.2 -- webpack --config ./azion/webpack.config.js -o ./worker --mode production || exit $? ;",
        "env": "./azion/webdev.env",
        "output-ctrl": "on-error"
    }
}
